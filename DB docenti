alter table dati_amministrativi rename column incarico_aggunntivo to incarico_aggiuntivo


alter table dati_amministrativi add tipo_contratto varchar2 (50);

update dati_amministrativi set tipo_contratto='indeterminato';

update dati_amministrativi set stipendio= stipendio*1.1 where incarico_aggiuntivo != 'nessuno';

update dati_amministrativi set anni_servizio=anni_servizio+1;

select cognome,nome from docenti where nome like 'MARIA%'

update dati_amministrativi set incarico_aggiuntivo='Primo collaboratore' 
where dati_amministrativi.id in (select docenti.id from docenti,dati_amministrativi where cognome='AMATO')
update dati_amministrativi set stipendio=stipendio+100 where dati_amministrativi.id in (select docenti.id from docenti,dati_amministrativi where cognome='AMATO')

update dati_amministrativi set incarico_aggiuntivo='Corso di  recupero di fisica' where id in (select id from docenti,residenze where materia='Fisica' and regione='Piemonte')

update residenze set citta=(select comune from admin.comuni where provincia='CN' and rownum=1)    /*errore: ORA-00001: unique constraint (BORRA.RESIDENZEDOC_PK) violated*/

delete from residenze where citta='JANova'


